<div class="locationsearch">

  <%= form_tag(controller: 'events', action: 'search', method: "") do %>

    <div>Venue name or type e.g. cafes</br> <%= text_field_tag 'find'%></div>
    <div>Location e.g. San Francisco</br><%= text_field_tag 'place' %></div>

  <%= submit_tag 'Find' %>
